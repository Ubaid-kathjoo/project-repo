{
  "family": "node-redis-task",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "<your_account_id>.dkr.ecr.<region>.amazonaws.com/node-redis-app:latest",
      "portMappings": [
        {
          "containerPort": 8081,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "memory": "512",
  "cpu": "256",
  "networkMode": "awsvpc",
  "executionRoleArn": "<ecsTaskExecutionRole ARN>"
}
